{
  "name": "vehicle-tracking",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-google-maps/api": "^2.19.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.96",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "env-cmd": "^10.1.0",
    "google-map-react": "^2.2.1",
    "react-bootstrap": "^2.9.0-beta.1",
    "react-icons": "^5.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f ./envs/.env react-scripts start",
    "build": "react-scripts build",
    "test": "env-cmd -f ./envs/.env react-scripts test --watchAll=false --coverage --coverageDirectory=coverage",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "docs": "typedoc --entryPointStrategy expand --tsconfig ./tsconfig.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "resetMocks": true,
    "preset": "ts-jest",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/shared/constants/",
      "<rootDir>/src/constants/",
      "<rootDir>/src/shared/interfaces/",
      "<rootDir>/src/shared/mocks/",
      "<rootDir>/src/assets/",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/reportWebVitals.ts",
      "<rootDir>/src/interfaces/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "lines": 80
      }
    },
    "coverageReporters": [
      "text",
      "cobertura",
      "html",
      "text-summary",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}